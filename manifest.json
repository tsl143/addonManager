{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "version": "0.0.1",
  "applications": {
    "gecko": {
      "id": "addonManager@web-ext-labs",
      "strict_min_version": "55.0"
    }
  },
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_title": "management"
  },
  "options_ui" : {
    "page"          : "index.html"
   ,"browser_style" : true
},
  "permissions": ["management"]
}